#!/bin/sh -e

if [ "$1" = purge ]; then

	rm -rf /var/lib/uemusiclibrary/cache
	rm -rf /var/log/uemusiclibrary

	if [ -x `which deluser` ]; then
		deluser uemusiclibrary 
	fi

fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0

